        <script>
        $(document).ready(function () {
            $.get('/api/getYearList', function (data) {
                if (data != "") {
                    var min = data.min_year;
                    var max = data.max_year;
                    var item = '';
                    while (min <= max) {
                        item += '<div class="item">' + min + '</div>';
                        min = min + 1;
                    }
                    $('#YearList').html(item);
                }
            });
            $("#add_another_vehicle").click(function() {
                if($(this).is(":checked")) {
                    $.get('/api/getYearList', function (data) {
                        if (data != "") {
                            var min = data.min_year;
                            var max = data.max_year;
                            var item = '';
                            while (min <= max) {
                                item += '<div class="item">' + min + '</div>';
                                min = min + 1;
                            }
                            $('#YearList1').html(item);
                        }
                });
                } else {
                    $('#YearList1').html('');
                }
            });
        });
        </script>
        <script>
            $('.decrease-count').click(function(){
                var dec_count = $(this).siblings('div').html();
                dec_count = parseInt(dec_count);
                dec_count = (dec_count == 0) ? dec_count : dec_count-1;
                $(this).siblings('div').html(dec_count);
                var input_id = $(this).siblings('div').attr('id')+'_input';
                $('#'+input_id).val(dec_count);
            });
            $('.increase-count').click(function(){
                var inc_count = $(this).siblings('div').html();
                inc_count = parseInt(inc_count);
                inc_count = inc_count+1;
                $(this).siblings('div').html(inc_count);
                var input_id = $(this).siblings('div').attr('id')+'_input';
                $('#'+input_id).val(inc_count);
            });
            $('#acc-1').change(function(){
                $('#accidentDiv').css('display','block');
            });
            $('#acc-2').change(function(){
                $('#accidentDiv').css('display','none');
            });
        </script>
