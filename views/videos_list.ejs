<!DOCTYPE html>
<html lang="en-US">
    <head>
        <% include partials/head.ejs%>
    </head>
    <body>
        <% include partials/header.ejs%>
        <section class="py-5 about-defy">
            <div class="container">
                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#library_video">Library of Videos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#products">Porducts</a>
                    </li>
                </ul>
                <div class="tab-content">
                    <div id="library_video" class="tab-pane active">
                        <div class="banner-inner featured-section" >
                            <div class="row mb-5">
                                <div class="col-lg-12">
                                    <h3>Library of Videos</h3>
                                    <div class="row">
                                        <% if(videoRes != ''){
                                            videoRes.forEach(function(res, i) { %>
                                                <div class="col-md-6">
                                                    <div class="about-video mt-4">
                                                        <iframe width="100%" height="315" src="<%= res.url %>" frameborder="0" allow="accelerometer; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                                                    </div>
                                                </div>
                                        <% })
                                        } else{ %>
                                            <div class="">
                                                <img src="/web_assets/assets/img/no_data.png">
                                            </div>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="products" class="tab-pane fade">
                        <h3 class="mb-4">Our Porducts</h3>
                        <div class="banner-inner featured-section" >
                            <div class="row justify-content-center mb-5">
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Auto</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-6.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Home</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-8.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Life</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-7.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Business</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-10.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Health care</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-9.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Real Estate</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-13.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>Non Profit</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-5.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-3">
                                    <div class="card featured-card border-style">
                                        <h4>More</h4>
                                        <div class="card-icon">
                                            <img src="/web_assets/assets/img/green-img/icon-green-11.png" width="100px"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Footer Ends -->
        <% include partials/footer.ejs%>
    </body>
</html> 