<script>
	$('.calculator-left-side-bar .list-group .list-group-item').click(function() {
		var sectionTo = $(this).attr('href');
		$('html, body').animate({
		  scrollTop: $(sectionTo).offset().top
		}, 0);
	});
	
	$(window).scroll(function() {    

		var scroll = $(window).scrollTop();

		if (scroll >= 200) {
			$(".calculator-left-side-bar").addClass("calculator_sidefixed");
		}
		else
			$(".calculator-left-side-bar").removeClass("calculator_sidefixed");
	});

